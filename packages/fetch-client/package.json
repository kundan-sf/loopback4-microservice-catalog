{
    "name": "@sourceloop/fetch-client",
    "version": "0.0.1",
    "description": "HTTP request adapter service",
    "keywords": [
        "loopback-microservice",
        "loopback"
    ],
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "engines": {
        "node": ">=10.16"
    },
    "scripts": {
        "build": "npm run clean && lb-tsc",
        "build:watch": "lb-tsc --watch",
        "lint": "npm run prettier:check && npm run eslint",
        "lint:fix": "npm run eslint:fix && npm run prettier:fix",
        "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
        "prettier:check": "npm run prettier:cli -- -l",
        "prettier:fix": "npm run prettier:cli -- --write",
        "eslint": "lb-eslint --report-unused-disable-directives .",
        "eslint:fix": "npm run eslint -- --fix",
        "pretest": "npm run build",
        "test": "echo \"No tests !\"",
        "posttest": "npm run lint",
        "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
        "clean": "lb-clean dist *.tsbuildinfo .eslintcache"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/sourcefuse/loopback4-microservice-catalog.git",
        "directory": "services/fetch-client"
    },
    "author": "Sourcefuse",
    "license": "MIT",
    "files": [
        "README.md",
        "dist",
        "src",
        "!*/__tests__"
    ],
    "private": true,
    "dependencies": {
        "@loopback/boot": "^4.1.2",
        "@loopback/context": "^4.1.2",
        "@loopback/core": "^3.1.2",
        "@loopback/openapi-v3": "^7.1.2",
        "@loopback/repository": "^4.1.2",
        "@loopback/rest": "^11.1.2",
        "@loopback/rest-explorer": "^4.1.2",
        "@loopback/service-proxy": "^4.1.2",
        "@opentelemetry/exporter-jaeger": "^1.0.1",
        "@opentelemetry/node": "^0.24.0",
        "@opentelemetry/plugin-dns": "^0.15.0",
        "@opentelemetry/plugin-http": "^0.18.2",
        "@opentelemetry/plugin-https": "^0.18.2",
        "@opentelemetry/plugin-pg": "^0.15.0",
        "@opentelemetry/plugin-pg-pool": "^0.15.0",
        "@opentelemetry/tracing": "^0.24.0",
        "@sourceloop/core": "^6.1.2",
        "@types/node-fetch": "^2.6.1",
        "@types/url-join": "^4.0.1",
        "loopback-connector-mysql": "^6.1.0",
        "loopback4-authentication": "^6.0.2",
        "loopback4-authorization": "^5.0.1",
        "node-fetch": "^2.6.7",
        "swagger-stats": "^0.99.2",
        "symlink-resolver": "",
        "tslib": "^2.0.0",
        "url-join": "^4.0.1"
    },
    "devDependencies": {
        "@istanbuljs/nyc-config-typescript": "^1.0.2",
        "@loopback/build": "^8.1.2",
        "@loopback/eslint-config": "^12.0.4",
        "@loopback/testlab": "^4.1.2",
        "@types/node": "^12.20.47",
        "dotenv": "^16.0.0",
        "dotenv-extended": "^2.9.0",
        "eslint": "^8.10.0",
        "nyc": "^15.1.0",
        "source-map-support": "^0.5.21",
        "typescript": "~4.6.3"
    }
}